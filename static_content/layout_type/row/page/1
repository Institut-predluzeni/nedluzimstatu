<div class="question every-item-except-first">Pobíral/a jste někdy sociální dávky?</div>
<p><span class="wpcf7-form-control-wrap radio-soc"><span class="wpcf7-form-control wpcf7-radio"><span class="wpcf7-list-item first"><input type="radio" name="radio-soc" value="ano" /><span class="wpcf7-list-item-label">ano</span></span><span class="wpcf7-list-item"><input type="radio" name="radio-soc" value="ne" /><span class="wpcf7-list-item-label">ne</span></span><span class="wpcf7-list-item last"><input type="radio" name="radio-soc" value="nechci odpovídat" /><span class="wpcf7-list-item-label">nechci odpovídat</span></span></span></span><br />
--></p>
<div class="et_pb_button_module_wrapper et_pb_button_3_wrapper et_pb_button_alignment_left mt-50">
    <a href="javascript:vyhodnotitFormular()" class="et_pb_button et_pb_custom_button_icon et_pb_button_3 main-cta et_pb_bg_layout_light" data-icon="$">Hotovo</a>
</div>
<p><script>
function vyhodnotitFormular() {
<!-- hide skryje vse, show ukaze jen tag li! -->
console.log('cteni formulare');
var radio_pes = document.forms.dotaznik['radio-pes'].value;
var radio_byt = document.forms.dotaznik['radio-byt'].value;
<!-- obec -->
if (radio_pes == 'ne' && radio_byt == 'ne') {jQuery('.et_pb_tab_3').hide();} else {jQuery('li.et_pb_tab_3').show();}
var radio_auto = document.forms.dotaznik['radio-auto'].value;
<!-- policie -->
if (radio_auto == 'ne') {jQuery('.et_pb_tab_2').hide();} else {jQuery('li.et_pb_tab_2').show();}
var radio_dp = document.forms.dotaznik['radio-dp'].value;
var radio_osvc = document.forms.dotaznik['radio-osvc'].value;
<!-- FU --> 
if (radio_dp == 'ne' && radio_osvc == 'ne') {jQuery('.et_pb_tab_0').hide();} else {jQuery('li.et_pb_tab_0').show();}
<!-- TODO test na zp pro osvc -->
var radio_up = document.forms.dotaznik['radio-up'].value;
var radio_soc = document.forms.dotaznik['radio-soc'].value;
<!-- CSSZ -->
if (radio_up == 'ne' && radio_osvc == 'ne' && radio_soc == 'ne') {jQuery('.et_pb_tab_1').hide();} else {jQuery('li.et_pb_tab_1').show();}
<!-- TODO UP tab chybí -->
if (radio_up == 'ne') {jQuery('.et_pb_tab_xx').hide();} else {jQuery('li.et_pb_tab_xx').show();}
<!-- vsechny ZP -->
checkbox_vzp = jQuery('input[name="checkbox-zp[]"][value="VZP"]:checked').length;
if (!checkbox_vzp && radio_osvc == 'ne') {jQuery('.et_pb_tab_4').hide();} else {jQuery('li.et_pb_tab_4').show();}
checkbox_ozp = jQuery('input[name="checkbox-zp[]"][value="OZP"]:checked').length;
if (!checkbox_ozp && radio_osvc == 'ne') {jQuery('.et_pb_tab_5').hide();} else {jQuery('li.et_pb_tab_5').show();}
checkbox_cpzp = jQuery('input[name="checkbox-zp[]"][value="ČPZP"]:checked').length;
if (!checkbox_cpzp && radio_osvc == 'ne') {jQuery('.et_pb_tab_6').hide();} else {jQuery('li.et_pb_tab_6').show();}
checkbox_vozp = jQuery('input[name="checkbox-zp[]"][value="VoZP"]:checked').length;
if (!checkbox_vozp && radio_osvc == 'ne') {jQuery('.et_pb_tab_7').hide();} else {jQuery('li.et_pb_tab_7').show();}
checkbox_zps = jQuery('input[name="checkbox-zp[]"][value="ZPŠ"]:checked').length;
if (!checkbox_zps && radio_osvc == 'ne') {jQuery('.et_pb_tab_8').hide();} else {jQuery('li.et_pb_tab_8').show();}
checkbox_rbp = jQuery('input[name="checkbox-zp[]"][value="RPB"]:checked').length;
if (!checkbox_rbp && radio_osvc == 'ne') {jQuery('.et_pb_tab_9').hide();} else {jQuery('li.et_pb_tab_9').show();}
checkbox_zpmv = jQuery('input[name="checkbox-zp[]"][value="ZPMV"]:checked').length;
if (!checkbox_zpmv && radio_osvc == 'ne') {jQuery('.et_pb_tab_10').hide();} else {jQuery('li.et_pb_tab_10').show();}
document.getElementById('formulare-navody').scrollIntoView()
}
</script></p>
