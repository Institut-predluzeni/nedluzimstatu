        zobrazitKrok(2);     
      
        jQuery('input[name="pojistovny[]"').each(function(index) {
        if (this.checked) {
           // load and store selected detail info before final submit
           detailOvm2(zdravotniPojistovny[this.value], this.value);
        }});
          
