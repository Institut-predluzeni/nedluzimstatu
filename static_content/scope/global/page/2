<p><script>
function vyhodnotitFormular() {
// hide skryje vse, show ukaze jen tag li! 
// pc a mobilni bloky jsou zvlast 
console.log('cteni formulare');
var radio_pes = document.forms.dotaznik['radio-pes'].value;
var radio_byt = document.forms.dotaznik['radio-byt'].value;
/* obec - vzdy zobrazeno 
if (radio_pes == 'ne' && radio_byt == 'ne') {
  jQuery('.et_pb_tab_3').hide();
  jQuery('.et_pb_accordion_item_3').hide();
} else {
  jQuery('li.et_pb_tab_3').show();
  jQuery('.et_pb_accordion_item_3').show();
}
*/
var radio_auto = document.forms.dotaznik['radio-auto'].value;
/* policie - vzdy zobrazeno
if (radio_auto == 'ne') {
  jQuery('.et_pb_tab_2').hide();
  jQuery('.et_pb_accordion_item_2').hide();
} else {
  jQuery('li.et_pb_tab_2').show();
  jQuery('.et_pb_accordion_item_2').show();
}
*/
var radio_dp = document.forms.dotaznik['radio-dp'].value;
var radio_osvc = document.forms.dotaznik['radio-osvc'].value;
var radio_nemovitost = document.forms.dotaznik['radio-nemovitost'].value;
// FU 
if (radio_dp == 'ne' && radio_osvc == 'ne' && radio_nemovitost == 'ne') {
  jQuery('.et_pb_tab_0').hide();
  jQuery('.et_pb_accordion_item_0').hide();
} else {
  jQuery('li.et_pb_tab_0').show();
  jQuery('.et_pb_accordion_item_0').show();
}
var radio_up = document.forms.dotaznik['radio-up'].value;
// vypnuto var radio_soc = document.forms.dotaznik['radio-soc'].value; 
var radio_soc = 'ne';
// CSSZ 
if (radio_up == 'ne' && radio_osvc == 'ne' && radio_soc == 'ne') {
  jQuery('.et_pb_tab_1').hide();
  jQuery('.et_pb_accordion_item_1').hide();
} else {
  jQuery('li.et_pb_tab_1').show();
  jQuery('.et_pb_accordion_item_1').show();
}
// TODO UP tab chybí
if (radio_up == 'ne') {
  jQuery('.et_pb_tab_xx').hide();
} else {
  jQuery('li.et_pb_tab_xx').show();
}
// vsechny ZP 
var checkbox_vzp = jQuery('input[name="checkbox-zp[]"][value="VZP"]:checked').length;
var checkbox_ozp = jQuery('input[name="checkbox-zp[]"][value="OZP"]:checked').length;
var checkbox_cpzp = jQuery('input[name="checkbox-zp[]"][value="ČPZP"]:checked').length;
var checkbox_vozp = jQuery('input[name="checkbox-zp[]"][value="VoZP"]:checked').length;
var checkbox_zps = jQuery('input[name="checkbox-zp[]"][value="ZPŠ"]:checked').length;
var checkbox_rbp = jQuery('input[name="checkbox-zp[]"][value="RBP"]:checked').length;
var checkbox_zpmv = jQuery('input[name="checkbox-zp[]"][value="ZP MV ČR"]:checked').length;
var zp_vybrano = false;
if (checkbox_vzp || checkbox_ozp || checkbox_cpzp || checkbox_vozp || checkbox_zps || checkbox_rbp || checkbox_zpmv) { zp_vybrano = true; }
if ((!checkbox_vzp && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_4').hide();
  jQuery('.et_pb_accordion_item_4').hide();
} else {
  jQuery('li.et_pb_tab_4').show();
  jQuery('.et_pb_accordion_item_4').show();
}
if ((!checkbox_ozp && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_5').hide();
  jQuery('.et_pb_accordion_item_5').hide();
} else {
  jQuery('li.et_pb_tab_5').show();
  jQuery('.et_pb_accordion_item_5').show();
}
if ((!checkbox_cpzp && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_6').hide();
  jQuery('.et_pb_accordion_item_6').hide();
} else {
  jQuery('li.et_pb_tab_6').show();
  jQuery('.et_pb_accordion_item_6').show();
}
if ((!checkbox_vozp && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_7').hide();
  jQuery('.et_pb_accordion_item_7').hide();
} else {
  jQuery('li.et_pb_tab_7').show();
  jQuery('.et_pb_accordion_item_7').show();
}
if ((!checkbox_zps && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_8').hide();
  jQuery('.et_pb_accordion_item_8').hide();
} else {
  jQuery('li.et_pb_tab_8').show();
  jQuery('.et_pb_accordion_item_8').show();
}
if ((!checkbox_rbp && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_9').hide();
  jQuery('.et_pb_accordion_item_9').hide();
} else {
  jQuery('li.et_pb_tab_9').show();
  jQuery('.et_pb_accordion_item_9').show();
}
if ((!checkbox_zpmv && zp_vybrano) || (radio_osvc == 'ne' && !zp_vybrano)) {
  jQuery('.et_pb_tab_10').hide();
  jQuery('.et_pb_accordion_item_10').hide();
} else {
  jQuery('li.et_pb_tab_10').show();
  jQuery('.et_pb_accordion_item_10').show();
}
jQuery('#kde-dluh').show();
jQuery('#formulare').hide();
jQuery('#formulare-navody').show();
// zobrazeni detailu prvniho dostupneho tabu 
jQuery('#instituce .et_pb_tabs_controls > li:visible').first().click();
jQuery('#instituce_mobil > div:visible > h5').first().click();
// prebarveni dalsi sekce
toggleDluhovePoradny()
document.getElementById('formulare-navody').scrollIntoView()
}
</script></p>
